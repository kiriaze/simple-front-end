<section class="dark">
	<div class="container">
		<form class="form-login" action="login" method="post">
			<div class="content">
				<h4>Log In</h4>
				<p>Lorem ipsum dolor sit amet.</p>
				<div class="simpleforms">
					<label for="username">Username</label>
